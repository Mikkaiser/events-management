<?xml version="1.0" encoding="utf-8" ?>
<Shell xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="App1.MainPage"
       xmlns:mainshell="clr-namespace:App1">

    <ShellItem>
        <ShellContent>
            <mainshell:InicialPage/>
        </ShellContent>
    </ShellItem>

    <FlyoutItem Title="Gerenciamento de eventos">
        <ShellContent ContentTemplate="{DataTemplate mainshell:InicialPage}" />
    </FlyoutItem>
    

</Shell>
